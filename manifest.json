
{
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "src/popup.html"
	},
	"content_scripts": [
		{
			"js": ["src/gpa.js"],
			"matches": ["http://powerschool.isb.ac.th/guardian/*", "https://powerschool.isb.ac.th/guardian/*"],
			"run_at": "document_end"
		}
	],
	"description": "Enhances PowerSchool Functionality",
	"icons": {
		"128": "icon.png",
		"16": "icon.png",
		"48": "icon.png"
	},
	"manifest_version": 2,
	"name": "ISB PowerSchool Enhancer",
	"permissions": ["https://powerschool.isb.ac.th/*", "http://powerschool.isb.ac.th/*"],
	"short_name": "ISBPWRSKLENH",
	"version": "1.0.0"
}
